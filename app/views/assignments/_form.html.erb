<%= simple_form_for ([@course, @assignment]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :type %>
    <%= f.input :due_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>


